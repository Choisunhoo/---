public class Ex3 {
    public static void main(String[] args) {
        char ch = 'A'; // 문자는 ''
        int i = 'A'; // int 에 문자를 저장할 경우 문자 코드 저장

        String str = ""; // 빈 문자열 (empty string)
        String str2 = "ABCD";
        String str3 = "123";
        String str4 = str2 + str3; // 문자열의 합침
        System.out.println(""+7+7); // 77 출력 
        System.out.println(7+7+""); // 14 출력
    }
}
